{
  "webfonts": {
    "src":       "app/fonts/svg/*.svg",
    "fontDest":  "app/fonts/",
    "sassDest":  "app/stylesheets/variables/_icon-glyphs.css.sass"
  },
  "sprites": {
    "src":       "app/images/sprites/",
    "imageDest": "app/images/sprites-{{density}}.png",
    "sassDest":  "app/stylesheets/variables/_sprites.css.scss"
  }
}

